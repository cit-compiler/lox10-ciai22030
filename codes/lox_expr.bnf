expression ::= equality
primary ::= NUMBER | STRING | "true" | "false" | "nil" | "(" expression ")"
unary ::= ( "!" | "-" ) unary
unary ::= ( "!" | "-" ) unary | primary
factor ::= factor ( "/" | "*" ) unary | unary
factor ::= unary ( ( "/" | "*" ) unary )*
expression ::= equality
equality ::= comparison ( ( "!=" | "==" ) comparison )*
comparison ::= term ( ( ">" | ">=" | "<" | "<=" ) term )* 
term ::= factor ( ( "-" | "+" ) factor )* 
factor ::= unary ( ( "/" | "*" ) unary )* 
unary ::= ( "!" | "-" ) unary | primary
primary ::= NUMBER | STRING | "true" | "false" | "nil" | "(" expression ")" 